        <script>
            function googleTranslateElementInit() {
                new google.translate.TranslateElement({ pageLanguage: 'en' }, 'google_translate_element');
            }
            function addToCart(prodId) {
                $.ajax({
                    url: '/add-to-cart?id=' + prodId,
                    method: 'get',
                    success: ((response) => {
                        Swal.fire({
                            position: 'top-end',
                            icon: 'success',
                            title: ' Item added to Cart',
                            showConfirmButton: false,
                            timer: 1500
                        })
                        let count = $('#cartCount').html
                        count = parseInt(count) + 1
                        $('#cartCount').html(count)
                    })
                })
            }
            function addToWishlist(prodId){
                $.ajax({
                    url: '/add-to-wishlist?id=' + prodId,
                    method: 'get',
                    success: ((response) => {
                        console.log(response);
                        if(!response.inc){
                            Swal.fire({
                            position: 'top-end',
                            icon: 'error',
                            title: ' Item Removed from Wishlist',
                            showConfirmButton: false,
                            timer: 1500
                        })
                            let count = $('#wishlist').html
                            count = parseInt(count) -1
                            $('#wishlist').html(count)
                            count = $('#wishlist1').html
                            count = parseInt(count) - 1
                            $('#wishlist1').html(count)

                        }else{
                            Swal.fire({
                            position: 'top-end',
                            icon: 'success',
                            title: ' Item added to Wishlist',
                            showConfirmButton: false,
                            timer: 1500
                        })
                            let count = $('#wishlist').html
                            count = parseInt(count) +1
                            $('#wishlist').html(count)
                            count = $('#wishlist1').html
                            count = parseInt(count) + 1
                            $('#wishlist1').html(count)

                        }
                        
                        
                    })
                })

            }
            function quickView(id){
                $.ajax({
                    url:'/quickview?id='+id,
                    method:'get',
                    success:((response)=>{
                        $("#image-1").attr("src",'/productimages/'+response.thumbnail);
                        $("#image-2").attr("src",'/productimages/'+response.images[0]);
                        $("#image-3").attr("src",'/productimages/'+response.images[1]);
                        $("#image-4").attr("src",'/productimages/'+response.images[2]);
                        
                        $("#image-1s").attr("src",'/productimages/'+response.thumbnail);
                        $("#image-2s").attr("src",'/productimages/'+response.images[0]);
                        $("#image-3s").attr("src",'/productimages/'+response.images[1]);
                        $("#image-4s").attr("src",'/productimages/'+response.images[2]);
                        $("#addToCart").attr("onclick",`addToCart('${response._id}')`);
                       
                        $('#title').html(response.title),
                        $('#fulldetails').html(response.fulldetails),
                        $('#oldPrice').html(Math.round(parseInt(response.price)*1.1))
                        $('#price').html(parseInt(response.price))
                    })
                })

            }
        </script>
